{
    "name": "salesforce-mc/marketing-cloud-sdk",
    "version": "1.0.0",
    "description": "The Salesforce Marketing Cloud PHP SDK",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/salesforce-marketingcloud/mcsdk-automation-php",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "league/oauth2-client": "2.6.1",
        "psr/cache": "1.0",
        "psr/simple-cache": "1.0",
        "psr/event-dispatcher": "1.0",
        "symfony/cache": "5.3.*",
        "symfony/dependency-injection": "4.4.*",
        "symfony/event-dispatcher": "5.3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.9"
    },
    "autoload": {
        "psr-4": { "SalesForce\\MarketingCloud\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "SalesForce\\MarketingCloud\\Test\\" : "test/" }
    }
}
